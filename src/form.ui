   <string notr="true">

/* nulloy-fluent-design insert start */
#mainWindow, #borderWidget {
  background: transparent !important;
  border: 1px solid #454545;
}

#mainWindow, #borderWidget, #splitter {
  border-radius: 8px;
}

#titleWidget {
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
}

* {
  font-family: "Segoe UI", "Inter", sans-serif;
  font-weight: normal !important;
}

#closeButton:pressed { background: #c42b1c; }
#closeButton:hover { background: #c42b1c; }
#minimizeButton:pressed, #menuButton:pressed { background: rgba(45, 45, 45, 175); }
#minimizeButton:hover, #menuButton:hover { background: rgba(45, 45, 45, 175); }

#prevButtonWrapper, #playButtonWrapper,
#stopButtonWrapper, #nextButtonWrapper {
  border-radius: 4px;
	margin: 3px 15px;
  padding: 16px;
}

#prevButtonWrapper:hover, #playButtonWrapper:hover,
#stopButtonWrapper:hover, #nextButtonWrapper:hover {
  background: #2b2b2b;
}

#titleWidget {
  background-color: #1e1e1eee;
}

#borderWidget, #splitter {
	background-color: #202020;
  background: url(mica-gradient.png);
}

#playlistWidget {
	color: #D4D4D4;
	qproperty-playing_text_color: #FFFFFF;
	qproperty-failed_text_color: #565659;
	background: rgba(0, 0, 0, 50);
	padding: 5;
	qproperty-file_drop_border_color: #5882CF;
	qproperty-file_drop_background: rgba(88, 130, 207, 50);
  border-radius: 8px;
}

NTrackInfoWidget #MiddleCenter {
	color: rgba(0,0,0,0);
  background: rgba(0,0,0,0);
}

#volumeSlider {
  border-radius: 4px;
  margin-right: 16px;
}

#volumeSlider::groove:horizontal {
	background: url(slider-inactive-bg.png);
	height: 20;
  border-radius: 4px;
}

#volumeSlider::handle:horizontal {
	width: 20px;
  height: 20px;
	background: url(slider-handle.png);
}

#volumeSlider::sub-page:horizontal {
	background: url(slider-active-bg.png);
}
/* nulloy-fluent-design insert end */
  
#titleLabel {
	color : #FFFFFF;
}

#coverWidget {
	background: transparent;
	border: 1px solid #252526;
}
#coverWidget:hover {
	border: 1px solid #5882CF;
}

#splitter::handle {
	background-image: url(splitter.png);
	background-repeat: no-repeat;
	background-position: center;
	padding: 1 0;
}


#playlistWidget::item {
	background: rgba(0, 0, 0, 50);
	border: 1 solid transparent;
  font-weight: normal !important;
  font-size: 16px;
}
#playlistWidget::item:alternate {
	background: #2D2D30;
}
#playlistWidget::item:selected {
	background: #2C3A52;
	border: 1 solid #4465A1;
  font-weight: normal !important;
}
#playlistWidget::item:alternate:selected {
	background: #343F52;
}
#playlistWidget::item:hover {
	border: 1 solid #5882CF;
}
#playlistWidget::item:selected:hover {
	background: #2B4169;
}
#playlistWidget::item:alternate:selected:hover {
	background: #3B4B69;
}

#playlistWidget QScrollBar {
	background: transparent;
	width: 15;
	margin-bottom: 15;
}
#playlistWidget QScrollBar::handle {
	background: #434346;
	margin-left: 5;
	min-height: 30px;
}
#playlistWidget QScrollBar::handle:hover {
	background: #6798F2;
}
#playlistWidget QScrollBar::add-line,
#playlistWidget QScrollBar::sub-line,
#playlistWidget QScrollBar::add-page,
#playlistWidget QScrollBar::sub-page {
	height: 0px;
	background: transparent;
}
#playlistWidget QSlider::sub-page:horizontal {
	background: #757575;
}

#mainWindow &gt; QSizeGrip, #sizeGrip {
	background-image: url(sizegrip.png);
	background-repeat: none;
	width: 17px;
	height: 17px;
}

#iconLabel {
	border-image: url(icon.png);
}

#shuffleButton {
	qproperty-icon: url(shuffle.png);
}
#repeatButton {
	qproperty-icon: url(repeat.png);
}
#repeatButton, #shuffleButton {
	border: 1 solid transparent;
}
#repeatButton:checked, #shuffleButton:checked {
	background: #4173D1;
}
#prevButton, #playButton, #stopButton, #nextButton {
	background: transparent;
}

#playButton, #stopButton,
#nextButton, #prevButton {
	border: none;
}

#prevButton {
	qproperty-icon: url(prev.png);
}
#stopButton {
	qproperty-icon: url(stop.png);
}
#playButton {
	qproperty-icon: url(play.png);
}
#nextButton {
	qproperty-icon: url(next.png);
}
#closeButton {
	qproperty-icon: url(close.png);
}
#minimizeButton {
	qproperty-icon: url(minimize.png);
}
#themeButton {
	qproperty-icon: url(theme.png);
}
#menuButton {
	qproperty-icon: url(menu.png);
}

#minimizeButton, #closeButton, #themeButton, #menuButton {
	border: 0;
}

NTrackInfoWidget {
	margin: 2;
}

NTrackInfoWidget QLabel {
	color: #FFFFFF;
	max-height: 14px;
	font-size: 13px;
  font-weight: 400;
  font-family: "Segoe UI Variable", "Segoe UI", "Inter", sans-serif;
	padding: 8;
}

#waveformSlider {
	qproperty-radius: 8;
  border-radius: 8px;
	qproperty-background: #0f0e1d;
	qproperty-wave_background: rgba(33, 30, 63, 50);
	qproperty-wave_border_color: #49428b;
	qproperty-progress_playing_background: #6c4679;
	qproperty-progress_paused_background: #2b2b2b;
	qproperty-playing_composition: Overlay;
	qproperty-paused_composition: Overlay;
	qproperty-file_drop_border_color: #5882CF;
	qproperty-file_drop_background: rgba(88, 130, 207, 50);
}</string>
